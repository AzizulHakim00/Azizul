#include<bits/stdc++.h>
  using namespace std;
  #define ll long long
  #define yes cout << "YES" << endl;
  #define no cout << "NO" << endl;
  const int mx = 2e5+123;
  int a[mx];
  vector<pair<ll, ll>>v;
  
  int main()
  {
    ios_base::sync_with_stdio(false);cin.tie(NULL);cout.tie(NULL);
    ll t = 1;
   // cin >> t;
    while(t--){
    double n;
      cin>>n;
      double ans =0;
      for(int i = 0 ; i < n; i++){
        double a; 
      cin>>a;
      ans += a;  
      }
       ans = ans/n; 
       std::cout << std::fixed << std::setprecision(12);
       cout<<ans<<endl;
       
 
      }
      return 0;
  }
  
