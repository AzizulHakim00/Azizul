#include<iostream>
#include<string>
using namespace std;
int main(){
    int n;
    string arr;
    cin>>n>>arr;
    int j;
    for(int i = 0 ; i < n; i++){
     if(arr[i] == 'n'){
            cout<<1<<" ";
     }
    }
    for(int i = 0; i < n; i++){

        if(arr[i] == 'z')cout<<0<<" ";
    }
return 0;
}
