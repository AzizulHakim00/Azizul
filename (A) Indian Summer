#include<bits/stdc++.h>
using namespace std;


#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

int main()
{
    optimize();

   
  
  int n; 
  cin>>n;
  vector<pair<string,string>> v(n);
  for(int i = 0 ; i < n; i++){
    string s,h;
    cin>>s>>h;
    v[i] = {s,h};


  }
  
  sort(v.begin(),v.end());
  int u = unique(v.begin(),v.end())-v.begin();
cout<<u;
}
    

