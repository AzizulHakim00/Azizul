#include<stdio.h>
int main(){
    unsigned long long int n,m,a;
    scanf("%llu %llu %llu",&n,&m,&a);
    unsigned long long int
     x,y,ans;
    if( n % a != 0){
        x = n/a;
        x = x+1;
    }
    else{
        x = n /a;
    }
    if(m% a != 0){
        y= m/a;
        y = y+1;
    }
    else{
        y = m/a;
    }

    ans = x*y;
    printf("%llu",ans);

return 0;}
